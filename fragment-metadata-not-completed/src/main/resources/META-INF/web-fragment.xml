<?xml version="1.0" encoding="UTF-8"?>
<web-fragment
        xmlns="http://java.sun.com/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-fragment_3_0.xsd"
        version="3.0"
        metadata-complete="false">

    <name>fragment2</name>

    <filter>
        <filter-name>fragment2Filter</filter-name>
        <filter-class>io.docbot.servlet3.fragment2.filter.FragmentFilter1</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>fragment2Filter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>

    <servlet>
        <servlet-name>fragment2Servlet1</servlet-name>
        <servlet-class>io.docbot.servlet3.fragment2.servlet.FragmentServlet1</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>fragment2Servlet1</servlet-name>
        <url-pattern>/fragmentServlet1</url-pattern>
    </servlet-mapping>


</web-fragment>
